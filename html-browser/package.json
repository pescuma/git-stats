{
	"name": "git-stats-html-browser",
	"version": "0.1.0",
	"description": "HTML application to view the results of git-stats",
	"homepage": "https://github.com/pescuma/git-stats",
	"main": "index.js",
	"scripts": {
		"dev": "webpack-dev-server --inline --hot",
		"build": "set NODE_ENV=production && webpack --progress --hide-modules && inliner -vm vue.html > build/index.html"
	},
	"dependencies": {
		"vue": "^1.0.0",
		"c3": "^0.4.0",
	  	"property-expr": "^1.3.1",
	  	"he": "^0.5.0"
	},
	"devDependencies": {
		"babel-core": "^5.8.25",
		"babel-loader": "^5.3.2",
		"babel-runtime": "^5.8.25",
		"css-loader": "^0.21.0",
		"jade": "^1.11.0",
		"style-loader": "^0.13.0",
		"stylus-loader": "^1.4.0",
		"template-html-loader": "0.0.3",
		"vue-hot-reload-api": "^1.2.0",
		"vue-html-loader": "^1.0.0",
		"vue-loader": "^6.0.0",
		"webpack": "^1.12.2",
		"webpack-dev-server": "^1.12.0",
		"inliner": "^1.6.1"
	},
	"author": "Ricardo Pescuma Domenecci",
